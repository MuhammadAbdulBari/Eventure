@model WebApplication1.Models.vendor

@{
    Layout = "_dashLayout";
}
<section class="home-section">
    <nav>
        <div class="sidebar-button">
            <i class='bx bx-menu sidebarBtn'></i>
            <span class="dashboard">Dashboard</span>
        </div>
        <div class="search-box">
            <input type="text" placeholder="Search...">
            <i class='bx bx-search'></i>
        </div>
        <div class="profile-details">
            <img src="https://t4.ftcdn.net/jpg/00/97/00/09/360_F_97000908_wwH2goIihwrMoeV9QF3BW6HtpsVFaNVM.jpg"
                 alt="profile">
            <span class="admin_name"> @ViewData["session"]</span>
            <i class='bx bx-chevron-down'></i>
        </div>

    </nav>



    <br><br>
    <div style="margin-top: 50px; margin-top:5%; margin-left: 7%;">
       <h3>Vendor Details</h3>

    </div>
    <div class="container">

        
        <dl class="row" style="margin-top:2%; padding:2%; margin-left:2%;">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Contact_Person)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Contact_Person)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Number)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Number)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Email)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Email)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Service_Type)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Service_Type)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Company_Name)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Company_Name)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Address)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Address)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.City)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.City)
        </dd>
    </dl>
</div>
    <div style=" margin-left:8%;">
    <a asp-action="VendorEdit" asp-route-id="@Model?.ID" class="btn btn-info">Edit</a> |
        <a asp-action="VendorView" class="btn btn-primary btn-xs">Back to List</a>
</div>

</section>